CREATE DATABASE cha CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci;

USE cha_casa_nova;

CREATE TABLE imagens (
    id INT PRIMARY KEY AUTO_INCREMENT,
    caminho VARCHAR(100),
    nome VARCHAR(100),
    ativo TINYINT(1) DEFAULT 1
) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci;

